<%=
    @page.css << "/css/page_view.css"

    @page.js << "/js/console.js"

    @page.js << "/js/poieticgen.js"
    @page.js << "/js/poieticgen/view_session.js"
    @page.js << "/js/poieticgen/zone.js"
    @page.js << "/js/poieticgen/viewer.js"
    @page.js << "/js/poieticgen/board.js"
    @page.js << "/js/page/view.js"
%>

<div data-role="page" data-fullscreen="true">
    <div data-role="header" data-id="head" data-position="fixed">
        <h1 class="username">Poietic Generator's view</h1>
        <a href="/" data-ajax="false" class="ui-btn-right logout">Back</a>
        <div data-role="navbar">
            <ul>
                <li><a href="#" id="view_start">History</a></li>
                <li><a href="#" id="view_now" class="ui-btn-active">Live</a></li>
            </ul>
        </div>
    </div>
    <div data-role="content">
        <div class="viewer">
            <canvas id="session-viewer"></canvas>
        </div>
    </div>
</div>
